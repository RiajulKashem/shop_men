[Unit]
Description=TPL_Django-uWSGI-instance
After=network.target postgresql-11.service

[Service]
User=centos
Group=nginx
WorkingDirectory=/opt/TPL_Django
Environment="PATH=/opt/TPL_Django/venv/bin"
ExecStart=/opt/TPL_Django/venv/bin/uwsgi --ini /opt/TPL_Django/uwsgi/uwsgi.ini
Restart=always
KillSignal=SIGQUIT
Type=notify
NotifyAccess=all

[Install]
WantedBy=multi-user.target